{
  "multipart": [
    {
        "apply": { "model": "morethings:block/rope/rope_knot"}
    },
    {   "when": { "north": "true" },
        "apply": { "model": "morethings:block/rope/rope_north"}
    },
    {   "when": { "south": "true" },
        "apply": { "model": "morethings:block/rope/rope_south"}
